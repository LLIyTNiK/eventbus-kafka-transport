{
  "name": "lliytnik/eventbus-transport-kafka",
  "description": "package for using kafka in eventbus",
  "type": "extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Fynjy",
      "email": "antony.ets@yandex.ru"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2.0",
    "lliytnik/eventbus": "dev-main",
    "lliytnik/kafka": "dev-main",
    "ext-rdkafka" : "*"
  },
  "autoload": {
    "psr-4": {
      "lliytnik\\eventbus\\transports\\": "src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/LLIyTNiK/eventbus-transport-kafka.git"
    },
    {
      "type": "path",
      "url": "../kafka",
      "options": {
        "symlink": true
      }
    }
  ]
}